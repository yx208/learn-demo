.search {
  width: 100%;
  height: 100%;
}

.search-input-wrap {
  display: flex;
  align-items: center;
  padding: 12rpx 0 12rpx 30rpx;
}

.search-input-wrap .si-container {
  flex: 1;
  display: flex;
  align-items: center;
  background-color: #f7f7f7;
  border-radius: 20px;
  padding: 0 8px;
}

.si-container .si-container-input {
  flex: 1;
  height: 60rpx;
  font-size: 14px;
  line-height: 1;
}

.search-input-wrap .si-cancel {
  line-height: 1;
  font-size: 16px;
  padding: 30rpx;
  color: #333;
}

.search .search-container {
  width: 100%;
  height: calc(100% - (110rpx));
  position: relative;
}

