{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "typeRoots": [
            "./node_modules/@types",
        ],
        "baseUrl": ".",
        "paths": {
            "/@/*": ["src/*"],
            "/@components/*": ["src/components/*"],
            "/@assets/*": ["src/assets/*"],
            "/@views/*": ["src/views/*"],
            "/@models/*": ["src/models/*"]
        },
        "allowSyntheticDefaultImports": true
    }
}