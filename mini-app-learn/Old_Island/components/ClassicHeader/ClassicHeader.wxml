<view class="classic-header">
  <view class="ch-left">
    <text class="ch-left-no">No.</text>
    <text class="ch-left-day">{{ (data.index > 9) ? (data.index) : ('0' + data.index) }}</text>
    <view class="ch-left-line"></view>
    <view class="ch-left-date">
      <view class="ch-left-month">{{ monthMap[date[1]] }}</view>
      <view class="ch-left-year">{{ date[0] }}</view>
    </view>
  </view>
  <view class="ch-right">
    <view class="ch-right-live" data-count="{{ data.fav_nums }}">
      <image class="ch-live-image" src="{{ likeIconMap[data.like_status] }}"></image>
    </view>
    <image class="ch-right-share" src="images/share.png"></image>
  </view>
</view>
